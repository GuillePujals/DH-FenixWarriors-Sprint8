<!DOCTYPE html>
<html>

<%- include ('../partials/head') %>
    <link rel="stylesheet" href="/css/listProducts.css">

    <body>
        <%- include ('../partials/header') %>
            <main class="main">

                <h1 class="titulos">Listado de productos</h1>

                <section class="listadoDeProductos">
                    <% casas.forEach (casa=> {%>
                        <% if (locals.userLogged.id==casa.user_id){ %>
                            <%- include ('../partials/product', {casa}) %>
                                <% } %>
                                <button class="profile_button">
                                    <a href="/products/<%= casa.id %>"> Detalle</a>
                                </button>
                                    <% } )%>
                </section>
                

            </main>
            <%- include ('../partials/footer') %>
    </body>

</html>