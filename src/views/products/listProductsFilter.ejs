<!DOCTYPE html>
<html>

<%- include ('../partials/head') %>
    <link rel="stylesheet" href="/css/listProducts.css">
    <body>
        <%- include ('../partials/header') %>
            <main class="main">
                
               <section class="listadoDeProductos">
                    <% casas.forEach (casa => {%>
                        <%- include ('../partials/product', {casa}) %>    

                        <button class="profile_button">
                                <a href="/cart/<%=casa.id%>"> Alquilar</a>
                        </button>           

                        <% } )%>

                     
                </section>

            </main>
            <%- include ('../partials/footer') %>
    </body>
</html>